{
  "name": "desilu",
  "version": "1.0.2",
  "main": "dist/index.js",
  "description": "Desilu is a Javascript library for making on-the-fly design changes to your website during development time.",
  "scripts": {
    "test": "jest",
    "copyFiles": "copyfiles --up 1 src/**/*.html src/**/*.css dist",
    "build": "npm run copyFiles && tsc",
    "clean": "rimraf dist",
    "watch": "watch \"npm run build\" src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkburg101/desilu.git"
  },
