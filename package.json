  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkburg101/desilu/issues"
  },
  "homepage": "https://github.com/jkburg101/desilu#readme",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "copyfiles": "^2.1.0",
    "jest": "^23.6.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@types/node": "^10.12.7",
    "a-color-picker": "^1.1.3",
    "eslint": "^5.7.0",
